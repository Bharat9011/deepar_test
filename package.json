{
  "name": "quickstart-web-js-npm",
  "version": "1.0.0",
  "description": "Quickstart project for DeepAR Web.",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && webpack --config webpack.config.js --mode production && cpx \"public/**/*\" dist && cpx \"node_modules/deepar/**/*\" dist/deepar-resources",
    "dev": "webpack serve --mode development --open --port 8888"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "cpx": "^1.5.0",
    "rimraf": "^6.1.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.0"
  },
  "dependencies": {
    "deepar": "^5.4.1"
  }
}
